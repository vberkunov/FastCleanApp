<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0"
            style="-fx-background-color: #d8edea;"
            xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.app.comtrollers.Start">
   <children>
      <VBox layoutX="252.0" layoutY="14.0" prefHeight="366.0" prefWidth="342.0" style="-fx-background-color: #e9f5f3;">
         <children>
            <Button fx:id="buttonStart" alignment="CENTER"
                    ellipsisString="" mnemonicParsing="false"
                    prefHeight="50.0" prefWidth="150.0"
                    style="-fx-background-color: #2145e4;" text="Старт"
                    textAlignment="CENTER" textFill="#dcdee5"
                    textOverrun="CLIP" onAction="#pressStart">
               <VBox.margin>
                  <Insets bottom="75.0" left="100.0" top="75.0" />
               </VBox.margin>
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Button>
            <Button fx:id="buttonUpload" alignment="CENTER"
                    minWidth="-Infinity" mnemonicParsing="false"
                    prefHeight="50.0" prefWidth="150.0"
                    style="-fx-background-color: #2145e4;"
                    text="Выгрузить" textFill="#f1eff5"
                    textOverrun="CLIP" onAction="#pressUpload">
               <VBox.margin>
                  <Insets left="100.0" />
               </VBox.margin>
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <ImageView id="ImageLogo"
                 fitHeight="150.0" fitWidth="200.0"
                 layoutX="14.0" layoutY="25.0"
                 pickOnBounds="true" preserveRatio="true"
                 AnchorPane.bottomAnchor="225.0" AnchorPane.leftAnchor="30.0"
                 AnchorPane.rightAnchor="436.0" AnchorPane.topAnchor="30.0">
         <image>
            <Image url="@../img/ardix_logo.png" />
         </image>
      </ImageView>
      <Text id="logoText" fill="#f5f1f1"
            layoutX="78.0" layoutY="223.0"
            strokeType="OUTSIDE" strokeWidth="0.0"
            text="FastClean" AnchorPane.bottomAnchor="169.822265625"
            AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="402.78125"
            AnchorPane.topAnchor="202.177734375">
         <font>
            <Font name="Corbel Bold" size="28.0" />
         </font>
         <effect>
            <DropShadow blurType="TWO_PASS_BOX" color="#0b0a0a" />
         </effect>
      </Text>
   </children>
</AnchorPane>
